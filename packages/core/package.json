{
  "name": "structive",
  "version": "1.4.1",
  "description": "Structure-driven template engine where structure defines meaning.",
  "main": "dist/structive.mjs",
  "module": "dist/structive.mjs",
  "types": "dist/structive.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/structive.mjs",
      "default": "./dist/structive.mjs"
    }
  },
  "scripts": {
    "build": "node node_modules/typescript/bin/tsc && rollup -c && node scripts/copy-loaders.js",
    "clean": "node -e \"require('fs').rmSync('dist', {recursive: true, force: true})\"",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:watch": "vitest --coverage",
    "preversion": "npm test",
    "version": "npm run clean && npm run build && git add -A dist"
  },
  "keywords": [
    "template",
    "structure-driven",
    "declarative",
    "web-components",
    "templating",
    "ui-framework"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "tsc": "^2.0.4",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.39.1",
    "globals": "^15.15.0",
    "jsdom": "^27",
    "rollup-plugin-dts": "^5.3.0",
    "tslib": "^2.8.1",
    "typescript-eslint": "^8.47.0",
    "vitest": "^3.1.1"
  }
}
